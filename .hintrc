{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=color-scheme]",
          "meta[name=theme-color]",
          "img[loading]"
        ]
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color",
          "scroll-behavior",
          "overscroll-behavior-y",
          "color-scheme",
          "padding-inline"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "manifest-exists": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}